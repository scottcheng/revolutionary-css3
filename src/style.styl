*
  margin 0
  padding 0
  border 0
  font-size 100%
  font inherit
  vertical-align baseline

transition()
  -webkit-transition arguments

border-radius(n)
  -webkit-border-radius n

lightred = #e9463c
red = #f00
darkgrey = #555

body
  font-family Telex, sans-serif
  color darkgrey
  background-image url('data:image/png;base64,')

a
  color inherit
  text-decoration none
  transition 0.25s
  &:hover
    color red

.step
  position relative
  width 900px
  padding 36px
  -webkit-box-sizing border-box
  font-size 48px
  line-height 1.5
  opacity .1
  transition opacity 0.5s

  &.active
    opacity 1

#title
  text-align center
  h1
    border-top 3px solid lightred
    border-bottom 3px solid lightred
    padding 1em 0
    text-transform uppercase
    span
      display inline-block
      margin
    .article
      letter-spacing 285px
      margin-right -285px
    .adj
      letter-spacing 39px
      margin-right -39px
      font-size 50%
      color lightred
    .noun
      font-size 570%
      line-height 100%
  .subtitle
    font-size 50%
    margin-top 3em

#impact
  p
    text-decoration line-through
    margin 2em 0
    // text-align center
    font-size 75%
    em
      font-size 150%

  .problem
    color lightred

  .animation em

  .visual em
    display inline-block
    background-color darkgrey
    overflow hidden
    padding 12px 27px
    border-radius 21px
    border 12px solid #fff
    color #fff
    text-shadow 0 0 10px #fff, 0 0 20px #fff, 0 0 30px red, 0 0 40px red, 0 0 70px red, 0 0 80px red, 0 0 100px #ff00de, 0 0 150px red
    box-shadow 0 0 21px rgba(0,0,0,.25)

#how
  h1
    font-size 550%
    text-align center
    text-transform uppercase

#text-stroke
  p
    font-size 200%
    -webkit-text-stroke-color lightred
    -webkit-text-stroke-width 3px